signature client_dce_rpc {
	ip-proto == tcp
	payload /^\x05(\x00|\x01)(\x00|\x01|\x02|\x03|\x04|\x05|\x06|\x07|\x08|\x09|\x0a|\x0b|\x0c|\x0d|\x0e|\x0f|\x11|\x12\x13)\x03/
}

signature server_dce_rpc {
	ip-proto == tcp
	payload /^\x05(\x00|\x01)(\x00|\x01|\x02|\x03|\x04|\x05|\x06|\x07|\x08|\x09|\x0a|\x0b|\x0c|\x0d|\x0e|\x0f|\x11|\x12\x13)\x03/
	requires-reverse-signature client_dce_rpc
	enable "dce_rpc"
}
